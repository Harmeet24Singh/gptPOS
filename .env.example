# Payment Terminal Configuration
# Copy this to .env.local and fill in your actual values

# Square Terminal Integration
SQUARE_ACCESS_TOKEN=your_square_access_token_here
SQUARE_TERMINAL_DEVICE_ID=your_square_terminal_device_id_here

# Stripe Terminal Integration  
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here

# Moneris Terminal Integration (Popular in Canada)
MONERIS_STORE_ID=your_moneris_store_id_here
MONERIS_API_TOKEN=your_moneris_api_token_here
MONERIS_TERMINAL_ID=your_moneris_terminal_id_here

# Chase Paymentech Terminal Integration
CHASE_MERCHANT_ID=your_chase_merchant_id_here
CHASE_API_KEY=your_chase_api_key_here
CHASE_TERMINAL_ID=your_chase_terminal_id_here
CHASE_API_URL=https://orbital1.paymentech.net/authorize

# PayPal Terminal Integration
PAYPAL_CLIENT_ID=your_paypal_client_id_here
PAYPAL_CLIENT_SECRET=your_paypal_client_secret_here

# Clover Terminal Integration
CLOVER_API_TOKEN=your_clover_api_token_here
CLOVER_MERCHANT_ID=your_clover_merchant_id_here

# General Settings
PAYMENT_PROCESSOR=demo  # Options: square, stripe, moneris, paypal, clover, demo
TERMINAL_TIMEOUT=30     # Seconds to wait for terminal response
CURRENCY=CAD           # Currency code (CAD for Canada, USD for US)